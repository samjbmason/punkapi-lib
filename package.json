    "craft beer",
    "api"
  ],
  "author": "Sam Mason <sam.jbmason@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/samjbmason/punkapi-lib/issues"
  },
  "homepage": "https://github.com/samjbmason/punkapi-lib#readme",
  "dependencies": {
    "lodash": "^4.17.11",
    "moment": "^2.24.0",
    "punkapi-db": "^2.5.0",
    "unique-random-array": "^1.0.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "coveralls": "^3.0.3",
    "istanbul": "^0.4.5",
    "mocha": "^6.0.2",
    "mocha-lcov-reporter": "^1.3.0",
    "should": "^13.2.3"
  }
