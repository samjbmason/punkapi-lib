{
  "name": "punkapi-lib",
  "version": "2.4.0",
  "description": "A javascript library for interacting with the punkapi-db",
  "main": "./dist/index.js",
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "babel src -d dist",
    "dev": "babel src -d dist -w",
    "test": "npm run build & mocha",
